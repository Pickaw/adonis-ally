{
  "name": "@adonisjs/ally",
  "version": "2.0.9",
  "description": "Social authenticator for AdonisJs Framework",
  "main": "index.js",
  "files": [
    "examples",
    "lib",
    "providers",
    "src",
    "instructions.js",
    "instructions.md"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc japa",
    "test:win": "node ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "commit": "git-cz",
    "lint": "standard"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "social-auth",
    "oauth"
  ],
  "author": "adonisjs,amanvirk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/mrm-preset": "^1.0.9",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "nyc": "^12.0.2",
    "pkg-ok": "^2.2.0",
    "semver": "^5.5.0",
    "standard": "^11.0.1"
  },
  "standard": {
    "global": [
      "use"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.1",
    "debug": "^3.1.0",
    "got": "^8.3.2",
    "lodash": "^4.17.10",
    "oauth": "^0.9.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-ally.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-ally/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-ally#readme",
  "nyc": {
    "exclude": [
      "test",
      "japaFile.js"
    ]
  }
}
